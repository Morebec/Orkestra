#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Morebec\Orkestra\Core\Infrastructure\DependencyInjection\OrkestraServiceContainer;
use Morebec\Orkestra\ProjectGeneration\Application\Console\ConsoleCommand\CompileModuleConsoleCommand;
use Symfony\Component\Console\Application;

$container = new OrkestraServiceContainer();
$container->build();

$app = $container->get(Application::class);
$app->run();


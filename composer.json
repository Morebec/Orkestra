{
    "name": "morebec/orkestra",
    "description": "Orkestra is an opiniated PHP framework used to quickly prototype and develop applications based on Symfony.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jwillp",
            "email": "jwilliam.perreault@morebec.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/Morebec/ValueObjects.git",
            "type": "git"
        },
        {
            "url": "https://github.com/Morebec/FileLocator.git",
            "type": "git"
        },
        {
            "url": "https://github.com/Morebec/Stringy",
            "type": "git"
        }
    ],
    "require": {
        "php": "^7",
        "beberlei/assert": "^3.2",
        "morebec/file-locator": "^2.0",
        "morebec/value-objects": "^1.0",
        "morebec/stringy": "^3.0",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/filesystem": "^4.4",
        "symfony/messenger": "^5.0",
        "symfony/yaml": "^4.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Morebec\\Orkestra\\": "src/"
        }
    },
    "scripts": {
        "check-outdated": "composer outdated -m"
    }
}
